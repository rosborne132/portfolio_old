{
    "name": "portfolio",
    "private": true,
    "description": "Tech portfolio",
    "version": "0.1.0",
    "author": "Robert Osborne",
    "dependencies": {
        "classnames": "^2.2.6",
        "gatsby": "^2.19.43",
        "gatsby-image": "^2.2.25",
        "gatsby-plugin-manifest": "^2.2.21",
        "gatsby-plugin-offline": "^3.0.13",
        "gatsby-plugin-react-helmet": "^3.1.11",
        "gatsby-plugin-sharp": "^2.2.29",
        "gatsby-source-filesystem": "^2.1.31",
        "prop-types": "^15.7.2",
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "react-helmet": "^5.2.1",
        "react-particles-js": "^2.7.0",
        "react-scroll": "^1.7.14",
        "styled-components": "^4.4.0",
        "styledoui": "^0.1.21"
    },
    "devDependencies": {
        "eslint-config-react-app": "^5.0.2",
        "husky": "^3.1.0",
        "prettier": "^1.5",
        "pretty-quick": "^2.0.1"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop -H 0.0.0.0 -p 8000",
        "format": "prettier --write src/**/*.{js,jsx}",
        "start": "npm run develop",
        "serve": "gatsby serve"
    },
    "husky": {
